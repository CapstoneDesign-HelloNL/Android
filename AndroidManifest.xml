        <activity android:name=".GroupNotice_write" />
        <activity android:name=".GroupNotice_detail" />
        <activity android:name=".GroupSchedule_detail" />
        <activity android:name=".GroupDetailActivity" />
        <activity android:name=".GroupAddActivity" />
        <activity android:name=".GroupRequestJoin" />
        <activity android:name=".SetupFragment" />
        <activity android:name=".AlertFragment" />
        <activity android:name=".SocialFragment" />
        <activity android:name=".GroupFragment" />
        <activity android:name=".GroupMainActivity" />
        <activity android:name=".GroupSearch" />
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity" />
    </application>
</manifest>